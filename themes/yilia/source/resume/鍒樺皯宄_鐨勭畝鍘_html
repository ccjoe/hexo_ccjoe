    $create(Sys.Extended.UI.TabPanel, {"headerTab":$get("__tab_Tab_tpTesting"),"ownerID":"Tab"}, null, {"owner":"Tab"}, $get("Tab_tpTesting"));
});
Sys.Application.add_init(function() {
    $create(Sys.Extended.UI.TabContainer, {"activeTabIndex":0,"clientStateField":$get("Tab_ClientState")}, {"activeTabChanged":ActiveTabChanged}, null, $get("Tab"));
});
Sys.Application.add_init(function() {
    $create(Sys.UI._UpdateProgress, {"associatedUpdatePanelId":"pTab","displayAfter":10,"dynamicLayout":true}, null, null, $get("upLoading"));
});
